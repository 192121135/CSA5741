echo "Enter the first number"
read a 
echo "Enter the second number"
read b
echo "Enter the third number"
read c
if [ $a -gt $b] && [$num1 -gt $c]
then
echo $a
elif [$b -gt $a] && [$b - gt $c]
then
echo $b
else
echo $c
fi
OUTPUT
Enter the first number:4
Enter the second number:12
Enter the third number:2
The greatest of given three numbers is 12
   
   
